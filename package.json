{
  "name": "FlavorTest",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "clean": "cd android && ./gradlew clean && cd ..",
    "clean-cache": "rm -rf $TMPDIR/react-*; rm -rf $TMPDIR/haste-*; rm -rf $TMPDIR/metro-*; watchman watch-del-all",
    "debugHun": "npx react-native run-android --variant hunDebug --appIdSuffix hun",
    "debugEng": "npx react-native run-android --variant engDebug --appIdSuffix eng",
    "debugRus": "npx react-native run-android --variant rusDebug --appIdSuffix rus"
  },
  "dependencies": {
    "react": "16.5.0",
    "react-native": "0.57.8",
    "react-native-config": "^1.2.1",
    "react-transform-hmr": "^1.0.4"
  },
  "devDependencies": {
    "babel-jest": "26.1.0",
    "jest": "26.1.0",
    "metro-react-native-babel-preset": "0.60.0",
    "react-test-renderer": "16.5.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
